 *  logic vs bit
 */

#include <cstdlib>
#include <iostream>

using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {

    int a,b,c,d;
    a=b=1;
    c=d=0;
    
    if(a||c++){cout<<"false"<<endl;}
    cout << c << endl;
    
    if(b|d++){cout<<"false"<<endl;}
    cout << d << endl;
    
    return 0;
}

